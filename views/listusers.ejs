<%- partial('navbar.ejs') %>

<div class="hero-unit">
  <h1>
    Our Awesome Users:
  </h1>
</div>

<div class="row-fluid">
  <div class="span8">
    <div>
      <h2>
        Current users
      </h2>

    <div class="error" style="background:green; color:white">
      <%= additionalMessage %>
    </div>

    <ul>
      <% for (var i = 0; i < users.length; i++) { %>
        <li>
          <a href="/user/<%=users[i]%>">
            <%= users[i] %>
          </a>
        </li>
      <% } %>
    </ul>

  </div>
</div>

